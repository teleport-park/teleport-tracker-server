{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "tabs",
      4
    ],
    "max-line-length": [
      true,
      160
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
